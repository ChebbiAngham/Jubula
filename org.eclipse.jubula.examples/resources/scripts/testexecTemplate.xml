<configuration>
    <project>YourProjectName</project>
    <version>1.0</version>
    <autconfig>YourAUTConfiguration</autconfig>
    <!-- or, if you're using autrun
    <autid>YourAUTID</autid>
     -->
    <dburl>YourDatabaseURL (visible via the preferences)</dburl>
    <!-- or, if you want to use the DBScheme and workspace (-data) to access the database
    <dbscheme>YourDBScheme</dbscheme>
     -->
    <dbuser>YourDatabaseUsername</dbuser>
    <dbpw>YourDatabasePassword</dbpw>
     <!-- the server argument is optional. If you enter it, you must have started a separate AUT Agent. You must then enter the hostname of the AUT Agent in the server parameter and you must also enter the port number for the AUT Agent in the port parameter. If you do not enter a server parameter, then an embedded AUT Agent will be started
    <server>YourAUTAgentHostname</server> -->
    <!-- the port parameter is optional if you do not enter a server parameter. In this case, you can either enter a port number. This number will be used for the embedded AUT Agent. If you enter neither a port number nor a server parameter, the embedded AUT Agent will be started on a dynamically chosen port number. You must enter a port number if you are using the server parameter
    <port>PortNumberForAUTAgent</port> -->
    <language>en_US</language>
    <testsuite>TestSuite</testsuite>
    <!-- or, if you're using Test Jobs
    <testjob>TestJob</testjob>
     -->
    <datadir><!-- e.g. <pathToQADocs>/data --></datadir>
    <resultdir><!-- e.g. <pathToQADocs>/results --></resultdir>
</configuration>
