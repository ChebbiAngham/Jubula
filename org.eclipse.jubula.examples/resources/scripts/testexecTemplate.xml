<configuration>
    <project>YourProjectName</project>
    <version>1.0</version>
    <autconfig>YourAUTConfiguration</autconfig>
    <!-- or, if you're using autrun
    <autid>YourAUTID</autid>
     -->
    <dburl>YourDatabaseURL (visible via the preferences)</dburl>
    <!-- or, if you want to use the DBScheme and workspace (-data) to access the database
    <dbscheme>YourDBScheme</dbscheme>
     -->
    <dbuser>YourDatabaseUsername</dbuser>
    <dbpw>YourDatabasePassword</dbpw>
    <server>YourAUTAgentHostname</server>
    <port>PortNumberForAUTAgent</port>
    <language>en_US</language>
    <testsuite>TestSuite</testsuite>
    <!-- or, if you're using Test Jobs
    <testjob>TestJob</testjob>
     -->
    <datadir><!-- e.g. <pathToQADocs>/data --></datadir>
    <resultdir><!-- e.g. <pathToQADocs>/results --></resultdir>
</configuration>
