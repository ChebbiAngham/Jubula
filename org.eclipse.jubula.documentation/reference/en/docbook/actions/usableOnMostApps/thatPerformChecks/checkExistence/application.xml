      </listitem>
      <listitem>
        <para>
          Parameters:
          
          <itemizedlist>
            <listitem>
              <para>
                <xref linkend="title"/>
              </para>
            </listitem>
            <listitem>
              <para>
                <xref linkend="operator"/>
              </para>
            </listitem>
            <listitem>
              <para>
                <xref linkend="exists"/>
              </para>
            </listitem>
          </itemizedlist>
        </para>
      </listitem>
    </itemizedlist>
  </para>
