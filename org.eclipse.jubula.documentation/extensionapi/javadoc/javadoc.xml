<project default="javadoc">
	<target name="javadoc">
		<property name="cp" refid="rc.extension.classpath"/>
		<echo message="Classpath is ${cp}"/>
		<property name="sp" refid="rc.extension.sourcepath"/>
		<echo message="Sourcepath is ${sp}"/>

		<javadoc 
			access="protected" 
			author="false" 
			classpathref="rc.extension.classpath"
			destdir="${outputdir}" 
			doctitle="GUIdancer External Swing Component API" 
			nodeprecated="false" 
			nodeprecatedlist="false" 
			noindex="false" 
			nonavbar="false" 
			notree="false" 
			packagenames="org.eclipse.jubula.rc.swing.swing.implclasses.table,
			com.bredexsw.guidancer.autserverbase.exception,
			com.bredexsw.guidancer.autserverbase.driver,
			com.bredexsw.guidancer.tools.constants,
			org.eclipse.jubula.rc.swing.swing.implclasses,
			com.bredexsw.guidancer.tools.objects.event,
			com.bredexsw.guidancer.autserverbase.implclasses,
			org.eclipse.jubula.rc.swing.swing.implclasses.tree" 
			source="1.4"
			sourcepathref="rc.extension.sourcepath"
			splitindex="true" 
			use="true" 
			version="false">
			<tag name="created" scope="all" />
		</javadoc>
	</target>
</project>
